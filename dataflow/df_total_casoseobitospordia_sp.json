{
	"name": "df_total_casoseobitospordia_sp",
	"properties": {
		"description": "Total de casos e óbitos por dia no estado de SP, Dados-covid-19-estado.csv",
		"folder": {
			"name": "covid_sp"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_csv_iso_format_generico",
						"type": "DatasetReference"
					},
					"name": "CasosPorDia"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "SelectColunas"
				},
				{
					"name": "FiltroCasos"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Data as string,",
				"          {Total de casos} as integer,",
				"          {Casos por dia} as integer,",
				"          {Óbitos por dia} as short,",
				"          {_c4} as string,",
				"          {_c5} as string,",
				"          {_c6} as string,",
				"          {_c7} as string,",
				"          {_c8} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> CasosPorDia",
				"CasosPorDia select(mapColumn(",
				"          Data,",
				"          Total_de_casos = {Total de casos},",
				"          Casos_por_dia = {Casos por dia},",
				"          Obitos_por_dia = {Óbitos por dia}",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SelectColunas",
				"SelectColunas filter(Casos_por_dia > 0 || Obitos_por_dia > 0) ~> FiltroCasos"
			]
		}
	}
}