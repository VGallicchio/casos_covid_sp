{
	"name": "df_total_casoseobitospordia_sp",
	"properties": {
		"description": "Total de casos e óbitos por dia no estado de SP.",
		"folder": {
			"name": "covid_sp"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_csv_iso_format_generico",
						"type": "DatasetReference"
					},
					"name": "CasosPorDia"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "SelectColunas"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Data as date 'dd/MM/yyyy',",
				"          {Total de casos} as integer,",
				"          {Casos por dia} as integer,",
				"          {Óbitos por dia} as integer,",
				"          {_c4} as string,",
				"          {_c5} as string,",
				"          {_c6} as string,",
				"          {_c7} as string,",
				"          {_c8} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> CasosPorDia",
				"CasosPorDia select(skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SelectColunas"
			]
		}
	}
}