{
	"name": "df_vacinometro",
	"properties": {
		"folder": {
			"name": "covid_sp"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_csv_format_generico",
						"type": "DatasetReference"
					},
					"name": "Vacinometro"
				},
				{
					"dataset": {
						"referenceName": "ds_csv_format_generico",
						"type": "DatasetReference"
					},
					"name": "Sample"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_generico_azuresql",
						"type": "DatasetReference"
					},
					"name": "SinkVacinometro"
				}
			],
			"transformations": [
				{
					"name": "OrdenaAlfa"
				},
				{
					"name": "JoinCodMun"
				},
				{
					"name": "SeleColunas"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {Município} as string,",
				"          Dose as string,",
				"          {Total Doses Aplicadas} as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 650,",
				"     ignoreNoFilesFound: false) ~> Vacinometro",
				"source(output(",
				"          Cod_IBGE as integer,",
				"          Municipio as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 650,",
				"     ignoreNoFilesFound: false) ~> Sample",
				"Vacinometro sort(asc({Município}, true)) ~> OrdenaAlfa",
				"OrdenaAlfa, Sample join({Município} == Municipio,",
				"     joinType:'inner',",
				"     broadcast: 'auto')~> JoinCodMun",
				"JoinCodMun select(mapColumn(",
				"          Cod_IBGE,",
				"          Dose,",
				"          Total_Doses_Aplicadas = {Total Doses Aplicadas}",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SeleColunas",
				"SeleColunas sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> SinkVacinometro"
			]
		}
	}
}