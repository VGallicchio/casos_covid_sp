{
	"name": "df_vacinometro",
	"properties": {
		"folder": {
			"name": "covid_sp"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_csv_format_generico",
						"type": "DatasetReference"
					},
					"name": "Vacinometro"
				},
				{
					"dataset": {
						"referenceName": "ds_csv_format_generico",
						"type": "DatasetReference"
					},
					"name": "Sample"
				},
				{
					"dataset": {
						"referenceName": "ds_generico_azuresql",
						"type": "DatasetReference"
					},
					"name": "DosesDB"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "OrdenaAlfa"
				},
				{
					"name": "JoinCodMun"
				},
				{
					"name": "SeleColunas"
				},
				{
					"name": "JoinDosesID"
				},
				{
					"name": "SelColunasSink"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {Município} as string,",
				"          Dose as string,",
				"          {Total Doses Aplicadas} as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 650,",
				"     ignoreNoFilesFound: false) ~> Vacinometro",
				"source(output(",
				"          Cod_IBGE as integer,",
				"          Municipio as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 650,",
				"     ignoreNoFilesFound: false) ~> Sample",
				"source(output(",
				"          ID as integer,",
				"          Dose as string,",
				"          Total_Doses_Aplicadas as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> DosesDB",
				"Vacinometro sort(asc({Município}, true)) ~> OrdenaAlfa",
				"OrdenaAlfa, Sample join({Município} == Municipio,",
				"     joinType:'inner',",
				"     broadcast: 'auto')~> JoinCodMun",
				"JoinCodMun select(mapColumn(",
				"          Cod_IBGE,",
				"          Dose,",
				"          Total_Doses_Aplicadas = {Total Doses Aplicadas}",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SeleColunas",
				"SeleColunas, DosesDB join(SeleColunas@Dose == DosesDB@Dose,",
				"     joinType:'inner',",
				"     broadcast: 'auto')~> JoinDosesID",
				"JoinDosesID select(mapColumn(",
				"          Cod_IBGE,",
				"          Dose = ID,",
				"          Total_Doses_Aplicadas = SeleColunas@Total_Doses_Aplicadas",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SelColunasSink"
			]
		}
	}
}