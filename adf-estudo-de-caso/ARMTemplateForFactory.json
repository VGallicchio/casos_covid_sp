			"type": "string",
			"defaultValue": "https://dlstudyvitor.dfs.core.windows.net"
		},
		"ls_dataset_covid_sp_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().baseURL}"
		},
		"ls_estudo_de_caso_lake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://estudodecasolake.dfs.core.windows.net/"
		},
		"ls_keyvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://akv-studyvitor.vault.azure.net/"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/pl_execute_transform_load')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Load",
						"type": "DatabricksNotebook",
						"dependsOn": [
							{
								"activity": "Transform",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"policy": {
							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"notebookPath": {
								"value": "@activity('Transform').output",
								"type": "Expression"
							}
						},
						"linkedServiceName": {
							"referenceName": "DBW_EstudoDeCaso",
							"type": "LinkedServiceReference"
						}
					},
					{
						"name": "Transform",
						"type": "DatabricksNotebook",
						"dependsOn": [],
						"policy": {
							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"notebookPath": {
								"value": "@pipeline().parameters.Notebook",
								"type": "Expression"
							}
						},
						"linkedServiceName": {
							"referenceName": "DBW_EstudoDeCaso",
							"type": "LinkedServiceReference"
						}
					}
				],
				"policy": {
					"elapsedTimeMetric": {},
					"cancelAfter": {}
				},
				"parameters": {
					"Notebook": {
						"type": "string"
					}
				},
				"folder": {
					"name": "covid-sp"
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/DBW_EstudoDeCaso')]"
			]
		},
